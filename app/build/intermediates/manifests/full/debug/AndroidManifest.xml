<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eoemobile.infos_assistant"
    android:versionCode="2"
    android:versionName="1.1.0" >

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="com.eoemobile.infos_assistant.eoeInfosAssistant"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.eoemobile.infos_assistant.System" />
        <activity android:name="com.eoemobile.infos_assistant.Hardware" />
        <activity android:name="com.eoemobile.infos_assistant.Software" />
        <activity android:name="com.eoemobile.infos_assistant.Runing" />
        <activity android:name="com.eoemobile.infos_assistant.ShowInfo" />
        <activity android:name="com.eoemobile.infos_assistant.FSExplorer" />
    </application>

</manifest>